<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans.xsd">
       
    <!--Setter injection-->
      <bean id="studentBean" class="com.tnsif.springcoreexample.Student">     
        
          <property name="name" value="Sharmila"></property>  
          
      </bean>
      
      <bean id="collegeBean" class="com.tnsif.springcoreexample.College">
         <property name="student" ref="studentBean"></property>
      </bean>
</beans>